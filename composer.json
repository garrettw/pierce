{
    "name": "garrettw/pierce",
    "description": "Pierce is an IRC bot framework written for PHP 5.5+.",
    "type": "library",
    "keywords": ["irc", "bot", "framework"],
    "homepage": "http://github.com/garrettw/pierce",
    "license": "LGPL-2.1",
    "authors": [
        {
            "name": "Garrett Whitehorn",
            "homepage": "http://garrettw.net/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "garrettw/Dice": "dev-master",
        "garrettw/noair": "dev-master",
        "monolog/monolog": "~1.10"
    },
    "require-dev": {
        "behat/behat": "~3.0",
        "phpdocumentor/phpdocumentor": "~2.8",
        "phpspec/phpspec": "~2.1"
    },
    "autoload": {
        "psr-4": {
            "Pierce\\": "src/",
            "": "vendor/"
        }
    },
    "autoload-dev": {
        "psr-4": { "Pierce\\Tests\\": "tests/" }
    }
}
